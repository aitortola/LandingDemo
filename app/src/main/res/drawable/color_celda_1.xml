<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- estado -->
    <item android:state_pressed="false" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <gradient android:endColor="@color/color_celda_landing_1"
                android:startColor="@color/color_celda_landing_1"
                android:angle="180" />
            <corners android:radius="0dp" />
            <stroke android:width="2px" android:color="#999999" />
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <gradient android:endColor="#4Dff8400"
                android:startColor="#4Dff8400"
                android:angle="180" />
            <corners android:radius="0dp" />
            <stroke android:width="2px" android:color="#999999" />
        </shape>
    </item>
</selector>


